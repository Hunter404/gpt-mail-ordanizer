{
    "Note": "COPY THIS FILE AND FILL IN YOUR CONFIGURATION IN appsettings.production.json REMEMBER TO NOT COMMIT THAT FILE.",
    "OpenAi": {
        "Model": "gpt-3.5-turbo",
        "ApiKey": "YOUR TOKEN"
    },
    "Imap": {
        "Server": "YOUR SERVER",
        "Port": 587,
        "Username": "YOUR EMAIL",
        "Password": "YOUR PASSWORD",
        "SecureSocketOptions": "StartTls"
    },
    "Mail": {
        "MaxBatchSize": 50,
        "Categories": [
            "Personal",
            "Work",
            "Spam",
            "Newsletters",
            "Social",
            "Purchases",
            "Other"
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    }
}
